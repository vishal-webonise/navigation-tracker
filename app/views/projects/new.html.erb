<h1>Project Details</h1>
<%= form_for(@projects, :validate => true) do |p| %>
  <div class="field">
    <%= p.label :Name  %><br />
    <%= p.text_field :name %>
  </div>
  <div class="field">
    <%= p.label :Domain %><br />
    <%= p.text_field :domain %>
  </div>
  <div class="field">
    <%= p.label :Domain_URL %><br />
    <%= p.text_field :domain_url %>
  </div>
  <div class="actions">
    <%= p.submit "Submit" %>
  </div>
<% end %>