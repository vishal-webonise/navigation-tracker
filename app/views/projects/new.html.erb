<h1>Project Details</h1>
<%= form_for(@project) do |p| %>
    <div class="field">
      <%= p.label :name  %><br />
      <%= p.text_field :name %>
    </div>
    <div class="field">
      <%= p.label :domain %><br />
      <%= p.text_field :domain %>
    </div>
    <div class="field">
      <%= p.label :git_url %><br />
      <%= p.text_field :git_url %>
    </div>
    <div class="field">
      <%= p.label :project_Management_tool  %><br />
      <%= p.text_field :project_mgt_tool %>
    </div>
    <div class="actions">
      <%= p.submit "Submit" %>
    </div>
<% end %>