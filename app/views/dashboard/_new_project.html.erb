<%#= @project.inspect %>
<%= form_for(@project, :url =>  create_project_users_path, :html => { :method => :post, :class => 'form-horizontal' }) do |f| %>
    <%= render 'shared/error_messages' , :object => f.object%>
    <div class="control-group">
      <%= f.label :name, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :name %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :domain_name, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :domain_name %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :domain_url, :class => 'control-label' %>
      <div class="controls">
        <div class="input-prepend">
          <span class="add-on">http://</span>
          <%= f.text_field :domain_url, :placeholder => 'testing.oklisten.com' %>
        </div>
      </div>
    </div>
    <div class="form-actions">
      <%= f.submit "Create", :class => 'btn btn-primary' %>
    </div>
<% end %>