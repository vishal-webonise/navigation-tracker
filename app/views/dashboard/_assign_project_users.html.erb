<%= form_tag(assign_project_users_from_dashboard_index_path, :method => "post", :id => "assign_project_users") do %>
  <%= label_tag(:user_project, "Project") %>
  <%= text_field_tag(:user_project) %>
  <%= label_tag(:project_users, "Users") %>
  <%= text_field_tag(:project_users, nil, :rel => @user_list ) %>
  <div class="form-actions">
    <%= submit_tag("Assign Users", :class => 'btn btn-primary') %>
  </div>
<% end %>