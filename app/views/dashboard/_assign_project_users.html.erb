<%= form_tag(assign_project_users_from_dashboard_index_path, :method => "post", :id => "assign_project_users", :class => 'form-horizontal') do %>
	<div class="control-group">
  	<%= label_tag(:user_project, "Select my Project", :class => 'control-label') %>
  	<div class="controls" style="display: inline-block; inline-block; margin-left: 10px;">
  		<%= text_field_tag(:user_project) %>
  	</div>
	</div>
	<div class="control-group">
  	<%= label_tag(:project_users, "Select Users to Assign", :class => 'control-label') %>
  	<div class="controls" style="display: inline-block; margin-left: 35px;">
  		<%= text_field_tag(:project_users, nil, :rel => @user_list ) %>
  	</div>
  </div>
  <div class="form-actions">
    <%= submit_tag("Assign Users", :class => 'btn btn-primary') %>
    <%= submit_tag "Cancel", :class => 'btn' %>
  </div>
<% end %>